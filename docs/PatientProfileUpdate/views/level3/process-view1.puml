@startuml Patient Profile Update (level 3 - 1)

autonumber
actor Admin as "Admin"
participant BE as "Backend Management" <<system>>
participant DB as "Database"

?o-> BE : PUT /api/patients
activate BE

BE -> DB : save patient

activate DB 

DB -> BE : patientProfileSaved 

deactivate DB 

<- BE : 200 Updated

deactivate BE

@enduml