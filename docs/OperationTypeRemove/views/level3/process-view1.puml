@startuml

autonumber
participant BE as "Backend Management" <<system (C4)>>
participant DB as "Database"

?o-> BE : PUT /api/operationTypes
activate BE

BE -> DB : get specialization

activate DB 

DB -> BE : specialization

deactivate DB 

BE -> DB : disable operationType

activate DB 

DB -> BE : operationType 

deactivate DB 

<- BE : 200 OK

deactivate BE

@enduml